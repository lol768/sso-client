<?xml version="1.0" encoding="ISO-8859-1" ?>
<!DOCTYPE taglib PUBLIC "-//Sun Microsystems, Inc.//DTD JSP Tag Library 1.2//EN" "http://java.sun.com/dtd/web-jsptaglibrary_1_2.dtd">

<taglib>

	<tlib-version>1.0</tlib-version>

	<jsp-version>1.2</jsp-version>

	<short-name>SSO</short-name>

	<uri>https://websignon.warwick.ac.uk</uri>

	<description>SSO tags</description>

	<tag>
		<name>loginlink</name>
		<tag-class>uk.ac.warwick.sso.client.SSOLoginLinkTag</tag-class>
		<description>Generates a link to login to the new SSO system</description>
		<attribute>
			<name>target</name>
			<required>false</required>
		</attribute>
	</tag>
	<tag>
		<name>logoutlink</name>
		<tag-class>uk.ac.warwick.sso.client.SSOLogoutLinkTag</tag-class>
		<description>Generates a link to logout of the new SSO system</description>
		<attribute>
			<name>target</name>
			<required>false</required>
		</attribute>
	</tag>

</taglib>
